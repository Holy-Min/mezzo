<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="#222222"
    android:layout_height="match_parent" >

    <LinearLayout
        android:id="@+id/allcontrol"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <Spinner
            android:id="@+id/test_spinner"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:entries="@array/mode" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:orientation="horizontal" >
            <Spinner
                android:id="@+id/islocation"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_gravity="center_vertical"
                android:entries="@array/location" />

            <Spinner
                android:id="@+id/isReadphone"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_gravity="center_vertical"
                android:entries="@array/readphone" />
        </LinearLayout>
    </LinearLayout>
    <FrameLayout
        android:id="@+id/fruting"
        android:layout_width="match_parent"
        android:layout_below="@+id/allcontrol"
        android:layout_height="match_parent">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >
                <LinearLayout
                    android:id="@+id/versionlayout"
                    android:layout_width="match_parent"
                    android:layout_below="@+id/allcontrol"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >
                    <TextView
                        android:id="@+id/version"
                        style="@style/version"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="10dp"
                        android:textIsSelectable="true" />

                    <TextView
                        android:id="@+id/adid"
                        style="@style/adid"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="10dp"
                        android:textIsSelectable="true" />

                </LinearLayout>
            <include layout="@layout/line" />
            <LinearLayout
                android:id="@+id/banner"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="visible" >
                <LinearLayout
                    android:layout_marginTop="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#333333"
                    android:orientation="horizontal" >

                    <EditText
                        android:id="@+id/banner_pub"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginLeft="10dp"
                        android:hint="PUBLISHER"
                        android:imeOptions="flagNoExtractUi"
                        android:inputType="number" />

                    <EditText
                        android:id="@+id/banner_media"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginRight="10dp"
                        android:imeOptions="flagNoExtractUi"
                        android:hint="MEDIA"
                        android:inputType="number" />
                </LinearLayout>
                <!--
                <Spinner
                    android:id="@+id/bannerStylespinner"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:layout_gravity="center_vertical"
                    android:entries="@array/bannerStyle" />
                    -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <TextView
                        style="@style/button"
                        android:background="#222222"
                        android:layout_gravity="left|center_vertical"
                        android:gravity="left|center_vertical"
                        android:layout_weight="2"
                        android:text="띠배너" />
                    <Button android:id="@+id/start_banner"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right|center_vertical"
                        android:text="start"/>
                    <Button android:id="@+id/stop_banner"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right|center_vertical"
                        android:text="stop"/>
                    <EditText
                        android:id="@+id/banner_sec"
                        android:layout_width="wrap_content"
                        android:imeOptions="flagNoExtractUi"
                        android:layout_height="wrap_content"
                        android:minWidth="140dp"
                        android:layout_gravity="right|center_vertical"
                        android:layout_marginRight="10dp"
                        android:hint="section"
                        android:inputType="number" />
                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_marginBottom="5dp"
                    android:background="#333333"
                    android:orientation="horizontal"
                    android:layout_height="wrap_content">
                    <RelativeLayout
                        android:layout_gravity="center"
                        android:id="@+id/bannerArea"
                        android:layout_margin="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:background="#555555"
                        android:orientation="horizontal" >
                    </RelativeLayout>
                </LinearLayout>

                <include layout="@layout/line" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/inter1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="visible" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:orientation="horizontal" >
