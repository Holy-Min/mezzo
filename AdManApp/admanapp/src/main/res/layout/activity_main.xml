                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="clickFull"
                        android:textSize="9sp"
                        android:id="@+id/clickFullAreaactivity"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_marginBottom="5dp"
                    android:background="#333333"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_height="wrap_content">
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="closeShow"
                        android:textSize="9sp"
                        android:id="@+id/closeShowactivity"/>
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="soundBtn"
                        android:textSize="9sp"
                        android:id="@+id/soundBtnShowactivity"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_marginBottom="5dp"
                    android:background="#333333"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_height="wrap_content">
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="clickBtn"
                        android:textSize="9sp"
                        android:id="@+id/clickBtnShowactivity"/>
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="skipBtn"
                        android:textSize="9sp"
                        android:id="@+id/skipBtnactivity"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_marginBottom="5dp"
                    android:background="#333333"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_height="wrap_content">
                    <RelativeLayout
                        android:layout_margin="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="200dp"
                        android:background="#555555"
                        android:orientation="horizontal" >
                    </RelativeLayout>
                </LinearLayout>
                <include layout="@layout/line" />
            </LinearLayout>

        </LinearLayout>
    </ScrollView>
</FrameLayout>
</RelativeLayout>